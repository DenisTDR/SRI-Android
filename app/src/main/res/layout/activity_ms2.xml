<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="tdr.bugcar_v1.MS2Activity">

    <TextView android:text="Comenzi pentru proba 2 a MS 2" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Proba 2"
        android:id="@+id/button"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/textView12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="proba2ms2"
        />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Proba 3"
        android:id="@+id/button4"
        android:layout_alignTop="@+id/button"
        android:layout_toRightOf="@+id/button"
        android:layout_toEndOf="@+id/button"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:onClick="proba3ms2"/>

</RelativeLayout>
